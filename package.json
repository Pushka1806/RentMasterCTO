{
  "name": "rentmaster2",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "description": "RentMaster2 - Event Equipment Rental Management System",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "setup": "npm install && npm run setup:env",
    "setup:env": "node -e \"const fs = require('fs'); if (!fs.existsSync('.env')) { fs.copyFileSync('.env.example', '.env'); console.log('✓ Created .env from .env.example'); console.log('⚠ Please edit .env with your Supabase credentials'); } else { console.log('✓ .env already exists'); }\"",
    "check": "node check-environment.js",
    "db:migrate": "cd supabase && bash run-migrations.sh",
    "db:status": "supabase db diff --schema public"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
